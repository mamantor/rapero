		function at(time, cb) {
	let later = time;
	if (!(time instanceof Date)) {
		const timestamp = Date.parse(time);
		if (!isNaN(timestamp)) {
			later = new Date(time);
		} else {
			throw error.create({
				msg: 'invalid date format',
				code: 'EINVALIDDATE'
			});
		}
	}

	const now = new Date();

	const timeDiff = later.getTime() - now.getTime();

	if (timeDiff <= 0) {
		throw error.create({
			msg: 'invalid Date. Cannot schedule in the past',
			code: 'EINVALIDDATE'
		});
	}
	return setTimeout(cb, timeDiff);
}

